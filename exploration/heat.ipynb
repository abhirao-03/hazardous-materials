    "        self.upper_bound = self.loc + self.radius\n",
    "\n",
    "def sink(scrub:scrubber, x, u):\n",
    "    \"\"\"\n",
    "    loc: location of sink\n",
    "    x: position\n",
    "    cot: concentration\n",
    "    \"\"\"\n",
    "    \n",
    "    scrub_loc = np.where((x >= scrub.lower_bound) & (x <= scrub.upper_bound))\n",
    "    u[scrub_loc] = (1 - scrub.efficiency) * u[scrub_loc]\n",
    "    \n",
    "    return u"
